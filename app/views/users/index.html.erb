<h1>Users</h1>
<table>
  <tr>
  	<th>First name</th>
  	<th>Last name</th>
  	<th>Email</th>
  	<th></th>
  </tr>
  
	<% @users.each do |user| %>
	  <tr>
	  	<td><%= user.first_name %></td>
	  	<td><%= user.last_name %></td>
	  	<td><%= user.email %></td>
	  	<td><%= link_to "Show", user %></td>
	  </tr>
	<% end %>
</table>